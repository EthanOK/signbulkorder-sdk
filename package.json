{
  "name": "signbulkorder-sdk",
  "version": "0.0.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EthanOK/signbulkorder-sdk.git"
  },
  "homepage": "https://github.com/EthanOK/signbulkorder-sdk",
  "keywords": [
    "typescript",
    "sdk"
  ],
  "author": "ethan",
  "license": "MIT",
  "description": "This is a sign bulk order sdk base seaport",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@ethersproject/bignumber": "5.8.0",
    "@opensea/seaport-js": "^4.0.6",
    "ethers": "^6.16.0",
    "ethers-v5": "npm:ethers@^5.8.0",
    "merkletreejs": "^0.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^22.5.1",
    "chai": "^4.3.4",
    "dotenv": "^16.4.5",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "mocha": "^9.2.2",
    "prettier": "^3.3.3",
    "standard-version": "^9.5.0",
    "ts-mocha": "^10.0.0",
    "tsup": "^8.3.5",
    "typedoc": "^0.26.11",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.18.0"
  },
  "lint-staged": {
    "{src,tests}/**/*.{js,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "prettier": "prettier --write \"{src,tests}/**/*.{js,ts,tsx,json,css,md}\"",
    "typedoc": "typedoc src/index.ts",
    "build": "tsup ./src/index.ts --format esm,cjs --dts",
    "release": "standard-version",
    "publish": "npm publish",
    "lint": "eslint \"{src,tests}/**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"{src,tests}/**/*.{js,ts,tsx}\" --fix",
    "test": "yarn run ts-mocha -p ./tsconfig.json -t 1000000 tests/**.spec.ts"
  }
}
